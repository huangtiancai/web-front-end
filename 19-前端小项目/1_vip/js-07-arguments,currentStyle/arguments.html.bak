 <body>
	<script>
		window.onload = function(){
			function fn(){
				var sum = 0;
				alert(arguments.length);
				alert(arguments[0]); //获取的是第一个参数1
				for(var i = 0;i < arguments.length;i++){
					sum += arguments[i];
				}
			}
			fn(1,2,3,8,9,6,5,2,1,4,6,9,7,4);
		}
	</script>
